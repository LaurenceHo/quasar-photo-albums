    "express": "^4.17.3",
    "express-throttle-bandwidth": "^1.0.1",
    "firebase-admin": "^11.2.0",
    "firebase-functions": "^4.0.1",
    "helmet": "^7.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.0",
    "@types/cookie-parser": "^1.4.3",
