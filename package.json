{
  "name": "vue-quasar-s3-albums",
  "version": "1.0.0",
  "description": "Quasar S3 photo album app",
  "productName": "Quasar S3 albums",
  "author": "Laurence Ho <bluegray.he@aol.com>",
  "scripts": {
    "serve": "quasar dev",
    "build": "quasar build",
    "lint": "eslint --ext .js,.ts,.vue ./ && prettier --write ./src/**/*.{js,ts,vue,scss}",
    "postinstall": "husky install",
    "firebase:deploy": "npm run build && firebase deploy",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run --coverage"
  },
  "dependencies": {
    "exifreader": "^4.3.1",
    "firebase": "^10.1.0",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.8.2",
    "pinia": "^2.0.14",
    "quasar": "^2.7.0",
    "vue": "^3.2.35",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.1",
    "@quasar/app-vite": "^1.0.0",
    "@quasar/extras": "^1.14.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.3.0",
    "@types/lodash": "^4.14.178",
    "@types/mapbox-gl": "^2.7.3",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-c8": "^0.31.4",
    "@vue/test-utils": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "dotenv": "^16.0.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-vue": "^9.0.1",
    "firebase-tools": "^12.4.7",
    "husky": "^8.0.1",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vitest": "^0.31.4"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": "^18",
    "npm": ">= 8",
    "yarn": ">= 1.21.1"
  },
  "prettier": {
    "semi": true,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5",
    "useTabs": false
  }
}
