{
  "name": "vue-quasar-s3-albums",
  "version": "0.0.1",
  "description": "Quasar S3 photo album app",
  "productName": "Quasar S3 albums",
  "author": "Laurence Ho <bluegray.he@aol.com>",
  "private": true,
  "scripts": {
    "serve": "quasar dev",
    "build": "quasar build",
    "lint": "eslint --ext .js,.ts,.vue ./ && prettier --write ./src/**/*.{js,ts,vue}",
    "postinstall": "husky install",
    "firebase-deploy": "npm run build && firebase deploy",
    "test:unit": "jest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.43.0",
    "@aws-sdk/client-s3": "^3.44.0",
    "@aws-sdk/credential-provider-cognito-identity": "^3.43.0",
    "@quasar/extras": "^1.12.2",
    "core-js": "^3.19.3",
    "lodash": "^4.17.21",
    "pinia": "^2.0.6",
    "quasar": "^2.3.4",
    "util": "^0.12.4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@pinia/testing": "0.0.8",
    "@quasar/app": "^3.2.5",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.7",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-vue": "^8.2.0",
    "firebase": "^9.6.1",
    "firebase-tools": "^9.23.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">=14"
  },
  "prettier": {
    "semi": true,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5",
    "useTabs": false
  }
}
