{
  "name": "vue-quasar-s3-albums",
  "version": "1.0.0",
  "description": "Quasar S3 photo album app",
  "productName": "Quasar S3 albums",
  "author": "Laurence Ho <bluegray.he@aol.com>",
  "scripts": {
    "serve": "quasar dev",
    "build": "quasar build",
    "lint": "eslint --ext .js,.ts,.vue ./ && prettier --write ./src/**/*.{js,ts,vue,scss}",
    "postinstall": "husky install",
    "firebase:deploy": "npm run build && firebase deploy",
    "test:unit": "jest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.48.0",
    "@aws-sdk/client-s3": "^3.48.0",
    "@aws-sdk/credential-provider-cognito-identity": "^3.48.0",
    "core-js": "^3.20.3",
    "exifreader": "^4.3.1",
    "firebase": "^9.6.4",
    "lodash": "^4.17.21",
    "pinia": "^2.0.9",
    "quasar": "^2.4.10"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@pinia/testing": "^0.0.9",
    "@quasar/app": "^3.2.9",
    "@quasar/extras": "^1.12.4",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.8",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-vue": "^8.3.0",
    "firebase-tools": "^10.1.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.2",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">=14"
  },
  "prettier": {
    "semi": true,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5",
    "useTabs": false
  }
}
