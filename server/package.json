{
  "name": "fastify-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.*",
    "package.json"
  ],
  "type": "module",
  "scripts": {
    "build:server": "bun esbuild.config.mjs",
    "format": "prettier --write src/ test/ --ignore-unknown --ignore-path .gitignore",
    "lint": "eslint --fix",
    "serverless:deploy": "serverless deploy",
    "serverless:deploy:prod": "serverless deploy --stage prod",
    "start:server": "rimraf dist && bun build:server && bun dist/index.js",
    "test:server": "vitest run --coverage",
    "types:server": "vue-tsc --build --force"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.679.0",
    "@aws-sdk/client-s3": "^3.679.0",
    "@aws-sdk/lib-dynamodb": "^3.679.0",
    "@fastify/auth": "5.0.1",
    "@fastify/cookie": "11.0.1",
    "@fastify/cors": "10.0.1",
    "@fastify/helmet": "12.0.1",
    "@fastify/multipart": "9.0.1",
    "@fastify/rate-limit": "10.1.1",
    "@fastify/throttle": "3.0.0",
    "aws-lambda": "^1.0.7",
    "dotenv": "^16.4.5",
    "electrodb": "3.0.1",
    "fastify": "5.0.0",
    "fastify-plugin": "5.0.1",
    "google-auth-library": "^9.14.2",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.2.10",
    "radash": "^12.1.0",
    "serverless-http": "^3.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/aws-lambda": "^8.10.145",
    "@types/jsonwebtoken": "^9.0.7",
    "@vitest/coverage-v8": "^2.1.4",
    "esbuild": "^0.24.0",
    "eslint": "^9.13.0",
    "form-auto-content": "^3.2.1",
    "globals": "^15.11.0",
    "rimraf": "^6.0.1",
    "serverless": "3.39.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-plugin-typescript": "^2.1.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": "^20",
    "npm": ">= 10",
    "yarn": ">= 4"
  },
  "lint-staged": {
    "*.ts": ["eslint --fix", "prettier --write src/ test/ --ignore-unknown --ignore-path .gitignore"]
  }
}
