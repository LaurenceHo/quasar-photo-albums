{
  "name": "fastify-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.*",
    "package.json"
  ],
  "type": "module",
  "scripts": {
    "build:server": "rimraf dist && bun esbuild.config.mjs",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:synth": "cdk synth PhotoAlbumDevStack",
    "cdk:deploy": "cdk deploy PhotoAlbumDevStack",
    "cdk:diff": "cdk diff PhotoAlbumDevStack",
    "format": "prettier --write src/ test/ --ignore-unknown --ignore-path .gitignore",
    "lint": "eslint --fix",
    "prepare:server:deploy": "rimraf dist && bun esbuild.lambda.mjs",
    "start:server": "bun build:server && bun dist/app/index.js",
    "test:server": "vitest run",
    "types:server": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.936.0",
    "@aws-sdk/client-s3": "^3.937.0",
    "@aws-sdk/lib-dynamodb": "^3.936.0",
    "@aws-sdk/s3-request-presigner": "^3.937.0",
    "@fastify/auth": "5.0.3",
    "@fastify/cookie": "11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "13.0.2",
    "@fastify/multipart": "9.3.0",
    "@fastify/rate-limit": "10.3.0",
    "@fastify/throttle": "3.0.2",
    "aws-cdk-lib": "^2.227.0",
    "aws-lambda": "^1.0.7",
    "dotenv": "17.2.3",
    "electrodb": "^3.5.0",
    "fastify": "5.6.2",
    "fastify-plugin": "5.1.0",
    "google-auth-library": "10.5.0",
    "jsonwebtoken": "^9.0.2",
    "radash": "^12.1.1",
    "serverless-http": "^3.2.0",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251121.0",
    "@eslint/js": "^9.39.1",
    "@tsconfig/node22": "^22.0.5",
    "@tsconfig/strictest": "^2.0.8",
    "@types/aws-lambda": "^8.10.159",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.19.1",
    "aws-cdk": "^2.1033.0",
    "aws-sdk-client-mock": "^4.1.0",
    "esbuild": "0.27.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "form-auto-content": "^3.2.1",
    "globals": "^16.5.0",
    "lint-staged": "16.2.7",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vitest": "4.0.13"
  },
  "engines": {
    "node": "^22",
    "npm": ">= 10",
    "yarn": ">= 4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  }
}
