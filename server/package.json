{
  "name": "fastify-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.*",
    "package.json"
  ],
  "type": "module",
  "scripts": {
    "build:server": "rimraf dist && bun esbuild.config.mjs",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:synth": "cdk synth PhotoAlbumDevStack",
    "cdk:deploy": "cdk deploy PhotoAlbumDevStack",
    "cdk:diff": "cdk diff PhotoAlbumDevStack",
    "format": "prettier --write src/ test/ --ignore-unknown --ignore-path .gitignore",
    "lint": "eslint --fix",
    "prepare:server:deploy": "rimraf dist && bun esbuild.lambda.mjs",
    "start:server": "bun build:server && bun dist/app/index.js",
    "test:server": "vitest run --coverage",
    "types:server": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.808.0",
    "@aws-sdk/client-s3": "^3.808.0",
    "@aws-sdk/lib-dynamodb": "^3.808.0",
    "@aws-sdk/s3-request-presigner": "^3.808.0",
    "@fastify/auth": "5.0.2",
    "@fastify/cookie": "11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "13.0.1",
    "@fastify/multipart": "9.0.3",
    "@fastify/rate-limit": "10.2.2",
    "@fastify/throttle": "3.0.2",
    "aws-cdk-lib": "^2.195.0",
    "aws-lambda": "^1.0.7",
    "dotenv": "^16.5.0",
    "electrodb": "^3.4.1",
    "fastify": "5.3.2",
    "fastify-plugin": "5.0.1",
    "google-auth-library": "^9.15.1",
    "jsonwebtoken": "^9.0.2",
    "radash": "^12.1.0",
    "serverless-http": "^3.2.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@tsconfig/node22": "^22.0.1",
    "@tsconfig/strictest": "^2.0.5",
    "@types/aws-lambda": "^8.10.149",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.17",
    "@vitest/coverage-v8": "^3.1.3",
    "aws-cdk": "^2.1014.0",
    "aws-sdk-client-mock": "^4.1.0",
    "esbuild": "0.25.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "form-auto-content": "^3.2.1",
    "globals": "^16.1.0",
    "lint-staged": "^15.5.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.3"
  },
  "engines": {
    "node": "^22",
    "npm": ">= 10",
    "yarn": ">= 4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  }
}
