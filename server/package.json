{
  "name": "fastify-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.*",
    "package.json"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "prettier --ignore-path .gitignore --write ./src ./test --ignore-unknown",
    "lint": "eslint --ext .ts ./src ./test --fix",
    "serverless:deploy": "serverless deploy",
    "serverless:deploy:prod": "serverless deploy --stage prod",
    "start:server": "rimraf dist && tsc && node dist/index.js",
    "test:server": "vitest run --coverage",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.616.0",
    "@aws-sdk/client-s3": "^3.616.0",
    "@aws-sdk/lib-dynamodb": "^3.616.0",
    "@fastify/auth": "^4.6.1",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/throttle": "^2.0.1",
    "aws-lambda": "^1.0.7",
    "dotenv": "^16.4.5",
    "electrodb": "^2.14.2",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "google-auth-library": "^9.11.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.2.7",
    "radash": "^12.1.0",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/aws-lambda": "^8.10.141",
    "@types/jsonwebtoken": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "form-auto-content": "^3.2.1",
    "rimraf": "^5.0.9",
    "serverless": "^3.39.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-plugin-typescript": "^2.1.5",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": "^20",
    "npm": ">= 10",
    "yarn": ">= 4"
  },
  "lint-staged": {
    "*.ts": ["eslint --ext .ts ./src ./test --fix", "prettier --ignore-path .gitignore --write ./src ./test --ignore-unknown"]
  }
}
