{
  "name": "fastify-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.*",
    "package.json"
  ],
  "type": "module",
  "scripts": {
    "build:server": "tsc",
    "format": "prettier --ignore-path .gitignore --write ./src ./test --ignore-unknown",
    "lint": "eslint --fix",
    "serverless:deploy": "serverless deploy",
    "serverless:deploy:prod": "serverless deploy --stage prod",
    "start:server": "rimraf dist && tsc && node dist/index.js",
    "test:server": "vitest run --coverage",
    "types:server": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.650.0",
    "@aws-sdk/client-s3": "^3.651.0",
    "@aws-sdk/lib-dynamodb": "^3.650.0",
    "@fastify/auth": "^4.6.1",
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/throttle": "^2.0.1",
    "aws-lambda": "^1.0.7",
    "dotenv": "^16.4.5",
    "electrodb": "^2.14.3",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "google-auth-library": "^9.14.1",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.2.10",
    "radash": "^12.1.0",
    "serverless-http": "^3.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/aws-lambda": "^8.10.145",
    "@types/jsonwebtoken": "^9.0.6",
    "@vitest/coverage-v8": "^2.1.0",
    "eslint": "^9.10.0",
    "form-auto-content": "^3.2.1",
    "globals": "^15.9.0",
    "rimraf": "^6.0.1",
    "serverless": "^3.39.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-plugin-typescript": "^2.1.5",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": "^20",
    "npm": ">= 10",
    "yarn": ">= 4"
  },
  "lint-staged": {
    "*.ts": ["eslint --fix", "prettier --ignore-path .gitignore --write ./src ./test --ignore-unknown"]
  }
}
